{
  "name": "trabea",
  "version": "0.1.0",
  "description": "A base compiler for Toga documentation.",
  "keywords": [
    "toga",
    "trabea",
    "compiler",
    "documentation",
    "docblock",
    "docs",
    "doc"
  ],
  "homepage": "https://github.com/togajs/trabea",
  "bugs": "https://github.com/togajs/trabea/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "dist/trabea.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/trabea"
  },
  "dependencies": {
    "handlebars": "^3.0.3",
    "handlebars-registrar": "^1.5.0",
    "lunr": "^0.5.11",
    "mtil": "^0.1.3",
    "require-glob": "^1.3.1",
    "through2": "^2.0.0",
    "vinyl": "^0.5.0",
    "vinyl-fs": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-eslint": "^3.1.23",
    "babel-istanbul": "^0.2.10",
    "coveralls": "^2.11.3",
    "eslint": "^1.0.0-rc-1",
    "expect": "^1.8.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "rimraf": "^2.4.1",
    "stream-array": "^1.1.0",
    "toga": "^0.5.1",
    "trifle": "^0.3.2",
    "tunic": "^0.4.0"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build-helpers": "babel src/helpers --out-dir dist/helpers",
    "build-static": "cp -R src/assets src/data src/partials dist/.",
    "build-trabea": "babel src/trabea.js --out-file dist/trabea.js ",
    "build": "npm run build-helpers && npm run build-static && npm run build-trabea",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/*.js test/*.js",
    "test": "babel-istanbul cover -i src/$npm_package_name.js ./node_modules/.bin/_mocha -- test/*.js",
    "watch": "nodemon --exec 'npm run test' --watch src --watch test"
  }
}
