{
  "name": "@toga/trabea",
  "version": "0.1.0",
  "description": "A theme engine for Toga documentation.",
  "public": true,
  "keywords": [
    "toga",
    "trabea",
    "compiler",
    "documentation",
    "docblock",
    "docs",
    "doc"
  ],
  "homepage": "https://github.com/togajs/trabea",
  "bugs": "https://github.com/togajs/trabea/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/trabea"
  },
  "dependencies": {
    "handlebars": "^3.0.2",
    "handlebars-registrar": "^1.3.1",
    "lunr": "^0.5.8",
    "mtil": "^0.1.3",
    "require-glob": "^1.2.0",
    "through2": "^0.6.5",
    "vinyl": "^0.4.6",
    "vinyl-fs": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.1.13",
    "coveralls": "^2.11.2",
    "eslint": "^0.20.0",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "nodemon": "^1.3.7",
    "stream-array": "^1.1.0",
    "toga": "^0.4.0",
    "toga-css": "^0.1.3",
    "toga-js": "^0.1.3",
    "toga-markdown": "^0.1.2",
    "toga-sample": "^0.1.0"
  },
  "scripts": {
    "build": "babel index.js --out-file dist/index.js",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint index.js test/*.js",
    "test": "npm run lint && npm run cover",
    "watch": "nodemon --exec 'npm run test' --watch index.js --watch test"
  }
}
